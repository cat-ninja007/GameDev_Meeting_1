var student = {
  name: "John",
  age: 15,
  gender: "male",
  interest: "coding",
}

// Cara panggil 1
console.log(student.name)
console.log(student.age)
console.log(student.gender)
console.log(student.interest)

// Cara panggil 2
console.log(student['name'])
console.log(student['age'])
console.log(student['gender'])
console.log(student['interest'])

//Membuat Object Block & Menghitungnya
var block = {
  length: 10,
  width: 5,
  height: 4,
}

function volume(){
  var vol = block.length*block.width*block.height
  return "The volume of the block is: " + vol
}

console.log(volume())


var car = {
  type: 'Tesla',
  model: 'Model 3',
  color: 'White',

  //Buat Object Method
  displayCar: function() {
    console.log('I have ' + this.type)
  }
}

var motor = {
  type: 'Yamaha',
  model: 'Jupiter Z',
  color: 'Merah',

  //Buat Object Method
  displayMotor: function() {
    console.log('I have ' + this.type)
  }
}




